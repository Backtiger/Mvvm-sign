<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:Mvvmsign.ViewModel"
                    xmlns:view ="clr-namespace:Mvvmsign.View">


    <DataTemplate DataType="{x:Type vm:MainVM}">
        <view:UCMain />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UcChartListVM}">
        <view:UcCahrtList />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UcCustomerVM}">
        <view:UCCustomer />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UcMakeChartVM}">
        <view:UcMakeChart />
    </DataTemplate>


</ResourceDictionary>